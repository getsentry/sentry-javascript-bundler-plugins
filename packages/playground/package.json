{
  "name": "@sentry-internal/bundler-plugin-playground",
  "version": "4.9.1",
  "license": "MIT",
  "private": true,
  "scripts": {
    "build:playground": "run-p build:rollup build:vite build:webpack build:esbuild",
    "build:rollup": "rollup --config rollup.config.mjs",
    "build:vite": "vite build --config vite.config.js",
    "build:webpack": "node build-webpack.js",
    "build:esbuild": "node build-esbuild.js",
    "build:smallNodeApp": "vite build --config vite.config.smallNodeApp.js",
    "clean": "run-s clean:build",
    "clean:all": "run-p clean:deps",
    "clean:build": "premove ./out",
    "clean:deps": "premove node_modules",
    "start:proxyLogger": "ts-node scripts/request-logger-proxy.ts"
  },
  "dependencies": {
    "@sentry/bundler-plugin-core": "4.9.1",
    "@sentry/integrations": "7.50",
    "@sentry/node": "7.50",
    "@types/express": "^4.17.13",
    "@types/http-proxy": "^1.17.9",
    "esbuild": "0.17.19",
    "express": "^4.18.1",
    "http-proxy": "^1.18.1",
    "rollup": "3.2.0",
    "vite": "3.0.0",
    "webpack": "5.74.0"
  },
  "devDependencies": {
    "premove": "^4.0.0"
  },
  "volta": {
    "extends": "../../package.json"
  }
}
